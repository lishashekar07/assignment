num = list(map(int, input("Enter the integer araray ").split()))
def majority_elements(nums):
    if not nums:
        return []

    # Potential candidates for the majority element
    candidate1, candidate2, count1, count2 = None, None, 0, 0

    # First pass: find potential candidates
    for num in nums:
        if num == candidate1:
            count1 += 1
        elif num == candidate2:
            count2 += 1
        elif count1 == 0:
            candidate1, count1 = num, 1
        elif count2 == 0:
            candidate2, count2 = num, 1
        else:
            count1 -= 1
            count2 -= 1

    # Reset counts for the second pass
    count1, count2 = 0, 0

    # Second pass: verify the candidates
    for num in nums:
        if num == candidate1:
            count1 += 1
        elif num == candidate2:
            count2 += 1

    # Check if candidates appear more than n/3 times
    result = []
    n = len(nums)
    if count1 > n // 3:
        result.append(candidate1)
    if count2 > n // 3:
        result.append(candidate2)

    return result


print("Elements appearing more than n/3 times:", majority_elements(num))
